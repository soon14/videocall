{
  "name": "videochat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run tsc && node ./build/server.js",
    "dev": "tsc && nodemon --watch build/server/ build/server/server.js",
    "compile": "tsc && webpack --config ./webpack.config.js && cp -r ./src/client/main ./src/client/build && cp ./src/client/room/style.css ./src/client/build/room",
    "tsc": "tsc",
    "serve": "webpack serve --config webpack.config.js",
    "deploy": "./ignored/deploy.sh"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/ws": "^8.5.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "uuid": "^8.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/node": "^14.6.0",
    "@types/uuid": "^8.3.4",
    "express": "^4.18.1",
    "nodemon": "^2.0.9",
    "typescript": "^4.5.4"
  }
}
